{
  "name": "indigohack-backend",
  "version": "1.0.0",
  "description": "Backend server for indigo hack 24",
  "main": "server.ts",
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "dev": "pnpm exec nodemon",
    "lint": "tsc",
    "build": "pnpm exec rimraf ./build && tsc --noEmit false",
    "start": "npm run build && ts-node build/server.ts",
    "db:migrate": "pnpm exec prisma migrate dev --name '${npm_config_name}'",
    "db:gen": "pnpm exec prisma generate",
    "db:push": "pnpm exec prisma db push",
    "db:seed": "ts-node src/script/seed.ts",
    "db:ui": "pnpm exec prisma studio"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.12",
    "@types/supertest": "^2.0.16",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-prettier": "9.1.0",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "jwks-rsa": "^3.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4",
    "prettier": "3.3.3",
    "prisma": "^5.17.0",
    "prisma-generator-fake-data": "^0.14.2",
    "rimraf": "^6.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Leo5661/indigohack-backend.git"
  },
